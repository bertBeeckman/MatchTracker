<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Standings" table="standings">
    <id name="idstandings" type="integer" column="idstandings">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="wins" type="string" column="wins" length="45" nullable="true"/>
    <field name="draws" type="string" column="draws" length="45" nullable="true"/>
    <field name="losses" type="string" column="losses" length="45" nullable="true"/>
    <field name="points" type="string" column="points" length="45" nullable="true"/>
    <many-to-one field="teams" target-entity="Teams">
      <join-columns>
        <join-column name="teams_id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="leagues" target-entity="Leagues" mapped-by="standingsstandings"/>
  </entity>
</doctrine-mapping>
