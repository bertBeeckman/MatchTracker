<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Standings" table="standings">
    <id name="idstandings" type="integer" column="idstandings">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <many-to-many field="teams" target-entity="Teams" inversed-by="standings">
      <join-table name="standings_has_teams">
        <join-columns>
          <join-column name="standings_id" referenced-column-name="idstandings"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="teams_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
    <many-to-many field="leagues" target-entity="Leagues" inversed-by="standingsstandings">
      <join-table name="leagues_has_standings">
        <join-columns>
          <join-column name="standings_idstandings" referenced-column-name="idstandings"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="leagues_id" referenced-column-name="id"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
